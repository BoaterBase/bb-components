{
  "name": "@boaterbase/bb-components",
  "version": "0.1.34",
  "description": "BoaterBase Web Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/BoaterBase/bb-components.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/bb-components/bb-components.js",
  "jsdelivr": "dist/bb-components/bb-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "deploy": "npm version --no-git-tag-version patch && npm run build && npm publish --access public"
  },
  "devDependencies": {
    "@stencil/core": "^1.11.2",
    "@types/node": "^13.9.1"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/router": "^1.0.1",
    "group-array": "^1.0.0",
    "mapbox-gl": "^1.11.1",
    "natural-orderby": "^2.0.3",
    "slugify": "^1.4.0",
    "stencil-head": "^1.1.0"
  }
}
